   "cell_type": "code",
   "execution_count": 18,
   "id": "4ee4815c-09f7-463d-bd5d-a05a227f11dd",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Alert Distribution:\n",
      "severity\n",
      "High        463\n",
      "Medium      299\n",
      "Low         126\n",
      "Critical    112\n",
      "Name: count, dtype: int64\n",
      "\n",
      "==================================================\n",
      "  WORKLOAD REDUCTION ANALYSIS\n",
      "==================================================\n",
      "  Total Alerts Generated  : 1000\n",
      "  Without AI (manual)     : 1000 alerts reviewed\n",
      "  Auto-closed by AI       : 425 alerts\n",
      "  Requires Analyst        : 575 alerts\n",
      "  Workload Reduction      : 42.5%\n",
      "==================================================\n"
     ]
    }
   ],
   "source": [
    "# â”€â”€ Workload Reduction Simulation â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€\n",
    "total_alerts = len(data)\n",
    "\n",
    "# Count alerts by severity\n",
    "severity_counts = data['severity'].value_counts()\n",
    "print(\"Alert Distribution:\")\n",
    "print(severity_counts)\n",
    "\n",
    "# Simulate analyst workload\n",
    "# Without AI: analyst reviews ALL alerts\n",
    "without_ai = total_alerts\n",
    "\n",
    "# With AI: analyst only reviews High + Critical\n",
    "# Low + Medium are auto-handled\n",
    "auto_closed = len(data[data['severity'].isin(['Low', 'Medium'])])\n",
    "requires_analyst = len(data[data['severity'].isin(['High', 'Critical'])])\n",
    "\n",
    "reduction_percent = (auto_closed / total_alerts) * 100\n",
    "\n",
    "print(f\"\\n{'='*50}\")\n",
    "print(f\"  WORKLOAD REDUCTION ANALYSIS\")\n",
    "print(f\"{'='*50}\")\n",
    "print(f\"  Total Alerts Generated  : {total_alerts}\")\n",
    "print(f\"  Without AI (manual)     : {without_ai} alerts reviewed\")\n",
    "print(f\"  Auto-closed by AI       : {auto_closed} alerts\")\n",
    "print(f\"  Requires Analyst        : {requires_analyst} alerts\")\n",
    "print(f\"  Workload Reduction      : {reduction_percent:.1f}%\")\n",
    "print(f\"{'='*50}\")"
