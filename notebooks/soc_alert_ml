   "cell_type": "code",
   "execution_count": 16,
   "id": "00af2b75-43a9-4dc5-afd1-52705ad2c7ae",
   "metadata": {},
   "outputs": [],
   "source": [
    "def suggest_remediation(features):\n",
    "    # Fix: pass as DataFrame with proper column names\n",
    "    input_df = pd.DataFrame([features], columns=['failed_logins', 'external_ip', 'data_transfer_mb', 'malware_flag', 'dns_beaconing'])\n",
    "    \n",
    "    prediction = model.predict(input_df)[0]\n",
    "    severity = le.inverse_transform([prediction])[0]\n",
    "\n",
    "    remediation_map = {\n",
    "        \"Critical\": [\n",
    "            \"[CRITICAL] IMMEDIATE ACTION REQUIRED\",\n",
    "            \"-> Isolate affected host from network\",\n",
    "            \"-> Block source IP at firewall\",\n",
    "            \"-> Reset all user credentials on affected system\",\n",
    "            \"-> Escalate to Tier 2 SOC analyst immediately\",\n",
    "            \"-> Preserve memory dump for forensic investigation\"\n",
    "        ],\n",
    "        \"High\": [\n",
    "            \"[HIGH] URGENT ACTION REQUIRED\",\n",
    "            \"-> Investigate source IP reputation\",\n",
    "            \"-> Reset credentials for affected user account\",\n",
    "            \"-> Review authentication logs for past 24 hours\",\n",
    "            \"-> Enable enhanced monitoring on affected host\",\n",
    "            \"-> Notify SOC team lead\"\n",
    "        ],\n",
    "        \"Medium\": [\n",
    "            \"[MEDIUM] MONITOR CLOSELY\",\n",
    "            \"-> Flag source IP for continued monitoring\",\n",
    "            \"-> Review user activity logs\",\n",
    "            \"-> Check for related alerts in past 6 hours\",\n",
    "            \"-> Document findings and keep watch\"\n",
    "        ],\n",
    "        \"Low\": [\n",
    "            \"[LOW] LOW PRIORITY\",\n",
    "            \"-> Log alert for record keeping\",\n",
    "            \"-> No immediate action required\",\n",
    "            \"-> Review during next scheduled audit\"\n",
    "        ]\n",
    "    }\n",
    "\n",
    "    print(\"=\" * 50)\n",
    "    print(\"  ALERT ANALYSIS RESULT\")\n",
    "    print(\"=\" * 50)\n",
    "    print(f\"  Predicted Severity : {severity}\")\n",
    "    print(\"=\" * 50)\n",
    "    print(\"  Recommended Actions:\")\n",
    "    for action in remediation_map[severity]:\n",
    "        print(f\"  {action}\")\n",
    "    print(\"=\" * 50)\n",
    "\n",
    "    return severity"
   ]
  },
